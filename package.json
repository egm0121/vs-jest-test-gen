{
  "name": "vs-jest-test-gen",
  "displayName": "Jest Test Gen",
  "description": "The fastest way to generate an initial jest unit test file from a js or ts source",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Testing",
    "Snippets"
  ],
  "keywords": [
    "jest", "test", "generator", "unittest","boilerplate"
  ],
  "activationEvents": [
    "onCommand:vs-jest-test-gen.generateForCurrentFile"
  ],
  "main": "./extension.js",
  "author": "egm0121",
  "license": "MIT",
  "contributes": {
    "commands": [
      {
        "command": "vs-jest-test-gen.generateForCurrentFile",
        "title": "Generate Jest tests"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  },
  "dependencies": {
    "jest-test-gen": "^1.3.2"
  },
  "bugs": {
    "url": "https://github.com/egm0121/vs-jest-test-gen/issues",
    "email": "egm0121@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/egm0121/vs-jest-test-gen.git"
  },
  "homepage": "https://github.com/egm0121/vs-jest-test-gen/blob/main/README.md",
  "icon": "assets/logo.png",
  "publisher": "com-egm0121"
}
